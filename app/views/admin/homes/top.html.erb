<div class="container w-75">
  <div class="background-board py-3 px-5 my-4">
  <div class="margin-y">
    <div class="d-flex align-items-center justify-content-between">
      <h4 class="h4-search mb-4">Tea Review一覧</h4>
    </div>
    <div class="col-10 mb-3 mx-auto">
    <table class='table table-hover table-light-0 margin-y'>
      <tbody>
        <tr>
          <td>
            <h5 class="mypage-border">
              <%= link_to admin_record_coffees_path do %>
                <i class="fa-solid fa-mug-saucer fa-lg" style="color: #511f1f;"></i> Coffee Review List
              <% end %>
            </h5>
          </td>
          <td class="text-muted">コーヒーレビュー一覧</td>
        </tr>
        <tr>
          <td>
            <h5 class="mypage-border">
              <%= link_to admin_record_tea_leaves_path do %>
                <i class="fa-brands fa-envira fa-rotate-90 fa-lg" style="color: #3c6417;"></i> Tea Review List
              <% end %>
            </h5>
          </td>
          <td class="text-muted">紅茶やお茶などレビュー一覧</td>
        </tr>
        <tr>
          <td>
            <h5 class="mypage-border">
              <%= link_to admin_users_path do %>
                <i class="fa-solid fa-user" style="color: #333333;"></i> User List
              <% end %>
            </h5>
          </td>
          <td class="text-muted">ユーザー一覧(編集・登録ステータス)</td>
        </tr>
      </tbody>
    </table>
  </div>
  </div>
</div>
